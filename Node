public class Node {
	private double nilai;
	private Node next;
	
	// inisialisasi Node	
	public Node (double nilai) {
		this.setNilai(nilai);
	}

	public double getNilai() {
		return nilai;
	}

	public void setNilai(double nilai) {
		this.nilai = nilai;
	}

	public Node getNext() {
		return next;
	}

	public void setNext(Node next) {
		this.next = next;
	}

	
